{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "build:lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "NODE_ENV=production nest start",
    "start:dev": "nest start --watch",
    "start:repl": "nest start --entryFile /repl",
    "start:debug": "nest start --debug --watch",
    "docker": "docker-compose build && docker-compose up -d",
    "lint": "prettier --check 'src/**/*.ts'",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./db/typeOrm.config.js",
    "typeorm:run-migrations:dev": "npm run typeorm migration:run -- -d ./db/typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./db/typeOrm.config.ts migration:generate ./db/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./db/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./db/typeOrm.config.js migration:revert",
    "typeorm:revert-migration:dev": "npm run typeorm -- -d ./db/typeOrm.config.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.2.9",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.9",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "@privy-io/server-auth": "^1.5.3",
    "@pushprotocol/restapi": "^1.4.38",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "ethers": "5.6",
    "express": "^4.18.2",
    "livekit-server-sdk": "^1.2.7",
    "nest-winston": "^1.9.4",
    "passport-strategy": "^1.0.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@types/express": "^4.17.21",
    "@types/passport-strategy": "^0.2.38",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescipt": "^1.0.0"
  }
}
